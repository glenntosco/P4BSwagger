    { "name": "Customer", "description": "Customer master data" },
    { "name": "Vendor", "description": "Vendor master data" },
    { "name": "Product", "description": "Product master data" },
    { "name": "Warehouse", "description": "Warehouse management" },
    { "name": "Account", "description": "Chart of accounts" },
    { "name": "SalesQuote", "description": "Sales quotes" },
    { "name": "SalesOrder", "description": "Sales orders" },
    { "name": "Invoice", "description": "Customer invoices" },
    { "name": "InventoryDelivery", "description": "Inventory deliveries (shipments)" },
    { "name": "PurchaseOrder", "description": "Purchase orders" },
    { "name": "VendorBill", "description": "Vendor bills" },
    { "name": "InventoryReceipt", "description": "Inventory receipts" },
    { "name": "JournalEntry", "description": "GL journal entries" },
    { "name": "PrintReport", "description": "Report generation" },
    { "name": "Inventory", "description": "Inventory levels and stock management" }
