    "/api/Inventory/GetAll": {
      "get": {
        "tags": ["Inventory"],
        "summary": "Get all inventory locations with stock levels",
        "operationId": "Inventory_GetAll",
        "description": "Returns inventory levels for all products across all warehouses. Includes product details, warehouse information, and current stock quantities.",
        "parameters": [
          { "name": "warehouseId", "in": "query", "schema": { "type": "string", "format": "uuid" }, "description": "Filter by warehouse ID" },
          { "name": "productId", "in": "query", "schema": { "type": "string", "format": "uuid" }, "description": "Filter by product ID" },
          { "name": "sku", "in": "query", "schema": { "type": "string" }, "description": "Filter by product SKU (partial match)" },
          { "name": "minQuantity", "in": "query", "schema": { "type": "number" }, "description": "Minimum quantity filter" }
        ],
        "responses": {
          "200": {
            "description": "List of inventory locations",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": { "$ref": "#/components/schemas/InventoryLocation" }
                }
